<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="description" content="{{ metaDesc }}">
<title>{{ pageTitle }}</title>
{% if canonical %}<link rel="canonical" href="{{ canonical }}">{% endif %}
{% if hreflangPt %}<link rel="alternate" hreflang="pt" href="{{ hreflangPt }}">{% endif %}
{% if hreflangEn %}<link rel="alternate" hreflang="en" href="{{ hreflangEn }}">{% endif %}
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Outfit:wght@200;300;400;500;600&family=Playfair+Display:ital,wght@0,400;0,500;1,400&family=Space+Mono:wght@400;700&display=swap" rel="stylesheet">
<link rel="icon" type="image/svg+xml" href="{{ site.site_favicon | default('/assets/img/fav-icon.svg') }}">
<link rel="stylesheet" href="/assets/css/styles.css">
